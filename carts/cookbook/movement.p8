pico-8 cartridge // http://www.pico-8.com
version 41
__lua__

function _init()
  px, py = 40, 40
end

function _update()
  if btn(0) then px -= 1 end
  if btn(1) then px += 1 end
  if btn(2) then py -= 1 end
  if btn(3) then py += 1 end
end

function _draw()
  cls()
  spr(1, px, py)
end

